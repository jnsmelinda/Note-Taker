{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "ignorePatterns": ["public"],
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "indent": [
            "error",
            4
        ],
        "max-len": [
            "error",
            140
        ],
        "comma-dangle": "off",
        "require-jsdoc": "off"
    }
}
